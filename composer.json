{
    "description" : "The \"Symfony Standard Edition\" distribution",
    "name": "root/pichon",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",

        "symfony/symfony": "2.5.*",
        "symfony/assetic-bundle": "~2.8",

        "doctrine/orm": "~2.4,<2.5",
        "doctrine/doctrine-bundle": "~1.2",

        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.3",

        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~2.3",
        "sensio/generator-bundle": "~2.3",

        "incenteev/composer-parameter-handler": "~2.0",

        "sonata-project/user-bundle": "~2.2",
        "sonata-project/admin-bundle": "2.3.*",
        "sonata-project/doctrine-orm-admin-bundle": "^2.3",
        "sonata-project/easy-extends-bundle": "^2.1",
        "sonata-project/media-bundle": "^2.3",
        "sonata-project/notification-bundle": "^2.3",
        "sonata-project/ecommerce": "dev-develop-deprecated",
        "sonata-project/comment-bundle": "~2.2@dev",
        "sonata-project/intl-bundle": "2.2.x-dev",
        "php-amqplib/php-amqplib": "2.6.2",
        "liip/monitor-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^1.7",
        "nelmio/api-doc-bundle": "^2.12",
        "michelf/php-markdown": "~1.4"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.3.9"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
