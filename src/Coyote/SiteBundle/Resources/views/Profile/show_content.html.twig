<div class="fos_user_user_show">
    <p id="titreconfig">{{'Information sur votre profil'|trans}} </p><br>
    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.name'|trans }}: {{ user.name }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <p>{{ 'profile.show.phone'|trans }} : {{ user.phone }}</p>
    {% if user.cell %}
        <p>{{ 'profile.show.cell'|trans }} : {{ user.cell }}</p>
    {% endif %}<br>
    <p>{{'profile.show.message'|trans}}</p>
</div>